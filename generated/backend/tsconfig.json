{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"declaration": true,
		"esModuleInterop": true,
		"lib": ["esnext"],
		"moduleResolution": "Node",
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"skipLibCheck": true,
		"types": ["node"],
		"module": "esnext",
		"target": "esnext"
	}
}
